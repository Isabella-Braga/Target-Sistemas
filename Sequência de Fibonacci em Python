print ('-' *3, 'Sequência de Fibonacci', '-' *3)
print (' ')

Valor = int(input('Insira um número: '))
print (' ')
T1 = 0
T2 = 1

print ('{} → {}'.format(T1, T2), end='')
Cont = 3

while Cont <= Valor:
   T3 = T1 + T2
   print (' → {}'.format(T3), end='')
   T1 = T2
   T2 = T3
   Cont += 1
print (' → FIM')
